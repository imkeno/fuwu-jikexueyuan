{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "background" : {
    "scripts" : ["jquery-1.12.1.min.js", "background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "cookies",
    "activeTab",
    "http://fuwu.jikexueyuan.com/"
  ]
}